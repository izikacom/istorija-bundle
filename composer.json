{
  "name": "dayuse/istorija-bundle",
  "description": "Symfony Bundle for dayuse/istorija integration",
  "license": "proprietary",
  "authors": [
    {
      "name": "Thomas Tourlourat",
      "email": "thomas@tourlourat.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "symfony/framework-bundle": "^5.4",
    "symfony/console": "^5.4",
    "dayuse/istorija": "1.0.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Dayuse\\IstorijaBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dayseuse\\Tests\\IstorijaBundle\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/izikacom/istorija.git"
    }
  ]
}
